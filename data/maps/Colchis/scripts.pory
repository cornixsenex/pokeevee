script Colchis_Script_SanctumTrigger {

	lockall

	//face the player and !
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceUp)
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_FaceUp)
	waitmovement (0)
	applymovement (OBJ_EVENT_JASON, Common_Movement_ExclamationMark)
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_ExclamationMark)
	waitmovement (0)

	///If Player hasn't interacted with either Lugia or Mareep just send him back
	if (!flag(FLAG_COLCHIS_HIDE_MAREEP) && !flag(FLAG_COLCHIS_HIDE_LUGIA)) {
			call (Colchis_Script_SanctumTriggerReturn)
	} else { //Player has cleared one of the two flags
		//Set Var as this will be the next step
		setvar (VAR_ARGO_STATE, 12)
		//Finished? Text
		call (Colchis_Script_SanctumTriggerEndIntro)
		//FAILED BOTH MEDEA AND JASON
		if (!flag(FLAG_COLCHIS_HIDE_MAREEP)) { 	
			
			//Medea goes to check Lugia
			call (Colchis_Script_SanctumTriggerMedeaExit)
			
			//Jason Battle (because you never interacted with Lugia)
			call (Colchis_Script_SanctumJasonMareep)
			
			//Medea returns You Monster, I wont help him Jason
			//Come my love let us care for each other's sorrows
			call (Colchis_Script_SanctumMedeaReturn)

			//If No Fleece then Jason says they go into the wild, else He will make her his queen in his land of Bulbus but leave the player to leave on his own
			//He turns to the player to thank him and meet him later at bulbus when she's calmed down
			//The exit
			//Setup Escape from Colchis
			//Fin
			//call (Colchis_Script_SetupEscapeFromColchis) - Called by MedeaReturn
		//Cleared Mareep no interact w/ Lugia check 4 Mareep
		} elif (!flag(FLAG_COLCHIS_HIDE_LUGIA)) { 
			closemessage
			//Jason asks for Fleece
			call (Colchis_Script_SanctumJasonMareep) //YES NO CLEAR OR ATTACK
			if (flag(FLAG_JASON_HAS_SHINYMAREEP)) {
				//Peaceful ending
				call (Colchis_Script_Exeunt)
			} else {
				//Jason says they go into the wild
				applymovement (OBJ_EVENT_JASON, Common_Movement_FaceLeft)
				waitmovement (0)

				load_field_pic(FIELD_PIC_JASON, 190, 98)
				msgbox(Colchis_Text_JasonSanctumWilds, MSGBOX_DEFAULT)
				destroy_field_pic (FIELD_PIC_JASON)
				
				//They exeunt
				//Setup Escape from Colchis
				//Fina
				call (Colchis_Script_SetupEscapeFromColchis)
			}
		} else { //both flags interacted means Medea goes to see Lugia and Jason checks Mareep
			call (Colchis_Script_SanctumTriggerMedeaExit)
			call (Colchis_Script_SanctumJasonMareep)
			call (Colchis_Script_SanctumMedeaReturn)
			call (Colchis_Script_SetupEscapeFromColchis)
		}
	}
	releaseall
	end
}

