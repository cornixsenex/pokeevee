script Colchis_TempScript_WarpSanctum {
	setvar (VAR_ARGO_STATE, 11)
	warpsilent (MAP_COLCHIS, 34, 56)
	end
}

script Colchis_TempScript_SetupTestMedeaDance {
	warpsilent (MAP_COLCHIS, 51, 25)
	setvar (VAR_ARGO_STATE, 10)

	end
}

script Colchis_TempScript_TestMedeaDance {
	lockall
	call (Colchis_Script_MedeaDance)
	releaseall
	end
}

script Colchis_Script_SetupJail {
	removeobject (OBJ_EVENT_JASON)
	setobjectxyperm (OBJ_EVENT_JASON, 18, 13)
	setobjectmovementtype (OBJ_EVENT_JASON, MOVEMENT_TYPE_FACE_DOWN)
	addobject (OBJ_EVENT_JASON)
	showobjectat (OBJ_EVENT_JASON, MAP_COLCHIS)	
	end
}



script Colchis_Script_Jason {
	if (var(VAR_ARGO_STATE) == 9) {
		msgbox(Colchis_Text_JasonIntroDeclined, MSGBOX_DEFAULT)
		closemessage
		setobjectmovementtype (OBJ_EVENT_JASON, MOVEMENT_TYPE_FACE_LEFT_AND_RIGHT)
	}
	end
}


script Colchis_Script_Intro {

	lockall

	setvar (VAR_ARGO_STATE, 8)
	//Player approach Jason
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_ExclamationMark)
	waitmovement (0)
	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_IntroPlayer)
	waitmovement (0)
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceDown)
	waitmovement (0)

	//Jason: Let us set out to claim Shiny Mareep
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox (Colchis_Text_JasonIntro1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//Walk Up 8
	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_JasonAmbush)
	applymovement (OBJ_EVENT_JASON, Colchis_Movement_JasonAmbush)
	waitmovement (0)

	//Ambush

	//Alert Jason and Player
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_ExclamationMark)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_ExclamationMark)
	waitmovement (0)
	//Spawn Intro Grunts
	//Move intro grunts into position
	applymovement (OBJ_EVENT_INTRO_GRUNT_M1, Colchis_Movement_IntroGruntM1)
	applymovement (OBJ_EVENT_INTRO_GRUNT_M2, Colchis_Movement_IntroGruntM2)
	applymovement (OBJ_EVENT_INTRO_GRUNT_M3, Colchis_Movement_IntroGruntM3)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F1, Colchis_Movement_IntroGruntF1)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F2, Colchis_Movement_IntroGruntF2)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F3, Colchis_Movement_IntroGruntF3)
	waitmovement (OBJ_EVENT_INTRO_GRUNT_M1)
	applymovement (OBJ_EVENT_INTRO_GRUNT_M1, Common_Movement_WalkDown)
	applymovement (OBJ_EVENT_INTRO_GRUNT_M3, Common_Movement_WalkDown)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F1, Common_Movement_WalkUp)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F3, Common_Movement_WalkUp)
	waitmovement (OBJ_EVENT_INTRO_GRUNT_M1)
	applymovement (OBJ_EVENT_INTRO_GRUNT_M1, Common_Movement_FaceRight)
	applymovement (OBJ_EVENT_INTRO_GRUNT_M3, Common_Movement_FaceLeft)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F1, Common_Movement_FaceRight)
	applymovement (OBJ_EVENT_INTRO_GRUNT_F3, Common_Movement_FaceLeft)
	waitmovement (0)

	//Jason: We're ambushed
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox (Colchis_Text_JasonIntro2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceDown)
	waitmovement (0)
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox (Colchis_Text_JasonIntro3, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceUp)
	waitmovement (0)
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox (Colchis_Text_JasonIntro4, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//Magma M: Welcome to Colchis Jason of Argo
	load_field_pic(FIELD_PIC_CULTIST_F, 190, 98)
	msgbox (Colchis_Text_GruntIntro1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_CULTIST_F)

	//Magma F: Aeetes King of Colchis awaits you
	load_field_pic(FIELD_PIC_CULTIST_M, 190, 98)
	msgbox (Colchis_Text_GruntIntro2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_CULTIST_M)

	//Jason turns to Player and says
	//We have no choice we'll have to go with them	
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceDown)
	waitmovement (0)
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox (Colchis_Text_JasonIntro5, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceUp)
	waitmovement (0)
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox (Colchis_Text_JasonIntro6, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	closemessage 

	call (Colchis_Script_EnterThroneRoom)

	//Else: Battle flags N shit
	end

}

