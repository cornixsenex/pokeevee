script Colchis_Script_SanctumJasonCheckMareep {
	
	specialvar (VAR_RESULT, IsShinyMareepInParty)
	//If no then goto Fight
	if (var(VAR_RESULT) == FALSE) {
		call (Colchis_Script_SanctumJasonBattle)
	//If yes then YES NO
	} else {
		
		load_field_pic(FIELD_PIC_JASON, 190, 98)
		msgbox(Colchis_Text_JasonGiveMeMareep, MSGBOX_DEFAULT)
		destroy_field_pic (FIELD_PIC_JASON)
	
		msgbox(Colchis_Text_GiveJasonMareep, MSGBOX_YESNO)
		//If choose no then goto Fight
		if (var(VAR_RESULT) == FALSE) {
			call (Colchis_Script_SanctumJasonBattle)
		//Else Choose Shiny Mareep
		} else {
			special (ChoosePartyMon)
			waitstate
			//If nothing chosen then go back up top
			if (var(VAR_0x8004) == PARTY_NOTHING_CHOSEN) {
				call (Colchis_Script_SanctumJasonCheckMareep)
			//Else if something chose first
			} else {
				specialvar (VAR_RESULT, ScriptGetPartyMonSpecies)
				//Check if it's Mareep
				if (var(VAR_RESULT) != SPECIES_MAREEP) {
					call (Colchis_Script_SanctumJasonCheckMareep)
				//Next check if it's Shiny
				} else {
					specialvar (VAR_RESULT, ScriptGetPartyMonIsShiny)
					//Not Shiny? Back Up Top
					if (var(VAR_RESULT) != TRUE) {
						call (Colchis_Script_SanctumJasonCheckMareep)
					//YES YOU HAVE SHINY MAREEP
					} else {
						special (BufferMonNickname)
						msgbox (Colchis_Text_GaveShinyMareep, MSGBOX_DEFAULT)
						closemessage
						special (ReleaseChosenMon)
						//Set Flag or Var for Jason has Mareep now (will open Bulbus and the Boat)
						setflag (FLAG_JASON_HAS_SHINYMAREEP)
					}
				}
			}
		}
	}

	return
}

script Colchis_Script_SanctumJasonMareep {

	//Move Player to position
	call (Colchis_Script_SanctumApproachJason)

	//Check if you even have Mareep
	call (Colchis_Script_SanctumJasonCheckMareep)

	return
}

