//Track Garden Progress - VAR_TEMP_9
script CityClara_Script_IncrementGardenVar {
	switch (var(VAR_TEMP_9)) {
		case 0:
			setvar (VAR_TEMP_9, 1)
		case 1:
			setvar (VAR_TEMP_9, 2)
		case 2:
			setvar (VAR_TEMP_9, 3)
		case 3:
			setvar (VAR_TEMP_9, 4)
		case 4:
			setvar (VAR_TEMP_9, 5)
		case 5:
			setvar (VAR_TEMP_9, 6)
		case 6:
			setvar (VAR_TEMP_9, 7)
		case 7:
			setvar (VAR_TEMP_9, 8)
		case 8:
			setvar (VAR_TEMP_9, 9)
		default:
			msgbox("VAR_TEMP_9 OOB")
			closemessage
	}
	return
}	

