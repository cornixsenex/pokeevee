script SuperWeenieHutJr_EventScript_TicTacToe_Win {
	//increase wins
	if (var(VAR_TEMP_F) == 3) {
		setvar (VAR_SWHJ_STATE, 2)
		setvar (VAR_TEMP_F, 4)
	} elif (var(VAR_TEMP_F) == 2) {
		setvar (VAR_TEMP_F, 3)
	} elif (var(VAR_TEMP_F) == 1) {
		setvar (VAR_TEMP_F, 2)
	} elif (var(VAR_TEMP_F) == 0) {
		setvar (VAR_TEMP_F, 1)
	} else {
		msgbox ("TOO MANY WINS IDK")
	}
	//set relevant board flag
	if (var(VAR_TEMP_E) == 1) {
		setflag (FLAG_TEMP_1)
	}
	if (var(VAR_TEMP_E) == 2) {
		setflag (FLAG_TEMP_2)
	}
	if (var(VAR_TEMP_E) == 3) {
		setflag (FLAG_TEMP_3)
	}
	if (var(VAR_TEMP_E) == 4) {
		setflag (FLAG_TEMP_4)
	}
	//Lost txt
	msgbox  (SuperWeenieHutJr_Text_TicTacToe_Lost)
	closemessage
	end
}

