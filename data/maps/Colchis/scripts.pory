script Colchis_Script_LostKid {
	lockall
	faceplayer
	//If in jail say thanks for saving me and return to nanny
	if (!flag(FLAG_COLCHIS_RETURN_LOSTKID)) {
		msgbox(Colchis_Text_LostKid1, MSGBOX_DEFAULT)
		closemessage
		special (GetPlayerFacingDirection)
		if (var(VAR_RESULT) == DIR_NORTH) {
			applymovement (OBJ_EVENT_LOSTKID, Colchis_Movement_LostKidExit1)
		} else {
			applymovement (OBJ_EVENT_LOSTKID, Colchis_Movement_LostKidExit2)
		}
		waitmovement (0)
		//Persist Return Lost Kid
		setflag (FLAG_COLCHIS_RETURN_LOSTKID)
		
		removeobject (OBJ_EVENT_LOSTKID)
		setobjectxyperm (OBJ_EVENT_LOSTKID, 5, 46)
		setobjectmovementtype (OBJ_EVENT_LOSTKID, MOVEMENT_TYPE_WANDER_AROUND)
		addobject (OBJ_EVENT_LOSTKID)
		showobjectat (OBJ_EVENT_LOSTKID, MAP_COLCHIS)
		
	//If with nanny say something like 'i love playing with my friends'
	} else {
		msgbox (Colchis_Text_LostKid2, MSGBOX_DEFAULT)
		closemessage
	}
	releaseall
	end
}

script Colchis_Script_DoorMan {
	lockall
	faceplayer

	//If his girl has returned (RETURN_LOSTKID) then he says like You brought us back I opened the door for you) and turns back to his girl else he's alone and says "You'll never get out of here unless I open the door for you and there's no way I'll do that...unless you could somehow calm my girl down. She won't come spend time with me because she's all stressed out about some lost kid. What about MY needs?
	if (!flag(FLAG_COLCHIS_RETURN_LOSTKID)) {
		msgbox (Colchis_Text_Doorman1, MSGBOX_DEFAULT)
		closemessage
		applymovement (OBJ_EVENT_DOORGUARD, Common_Movement_FaceDown)
	} else {
		if (var(VAR_ARGO_STATE) != 17) {
			setvar (VAR_ARGO_STATE, 17)
			msgbox (Colchis_Text_Doorman2, MSGBOX_DEFAULT)
			closemessage
			setmetatile (10, 63, METATILE_Colchis_JailDoorOpen_Bottom, FALSE)
			setmetatile (11, 63, METATILE_Colchis_JailDoorOpen_Bottom, FALSE)
			special (DrawWholeMapView)
		} else {
			msgbox (Colchis_Text_Doorman3, MSGBOX_DEFAULT)
			closemessage
		}
		applymovement (OBJ_EVENT_DOORGUARD, Common_Movement_FaceRight)
	}
	waitmovement (0)
	releaseall
	end
}

//At first she's stressed about the kid being missing like "This is terrible I lost one of the kids and I'm worried that the parents will figure out if I just swap her with another child... after she's with the guard down at his post and says something like "Oh thank you for finding that missing kid I feel like I can finally breath again I was so stressed out I hadn't been able to spend time with my man like I used to.
