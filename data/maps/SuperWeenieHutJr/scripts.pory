			call (SuperWeenieHutJr_EventScript_TicTacToe_GetCPUMove)
		} else {
			setvar (VAR_TEMP_B, VAR_RESULT)
		}
	} elif (var(VAR_RESULT) == 6) {
		if (var(VAR_TEMP_7) > 0) {
			call (SuperWeenieHutJr_EventScript_TicTacToe_GetCPUMove)
		} else {
			setvar (VAR_TEMP_B, VAR_RESULT)
		}
	} elif (var(VAR_RESULT) == 7) {
		if (var(VAR_TEMP_8) > 0) {
			call (SuperWeenieHutJr_EventScript_TicTacToe_GetCPUMove)
		} else {
			setvar (VAR_TEMP_B, VAR_RESULT)
		}
	} elif (var(VAR_RESULT) == 8) {
