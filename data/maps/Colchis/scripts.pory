script Colchis_Script_SanctumMedeaReturn {
	//Player and Jason notice Medea return
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceUp)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_FaceUp)
	waitmovement (0)
	
	applymovement (OBJ_EVENT_JASON, Common_Movement_ExclamationMark)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_ExclamationMark)
	waitmovement (0)

	//Offscreen Medea screams	
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox("OIMOIAY!\nOOIIMMOOIIAAYY!!", MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)
	closemessage

	//Enter Medea
	applymovement (OBJ_EVENT_MEDEA, Colchis_Movement_MedeaSanctumReturn)
	waitmovement (0)

	//Shes very upset at the player - lecture player
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaSanctumReturn1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)
	closemessage

	//Walks down to position
	if (var(VAR_TEMP_B) == 0) {	
		applymovement (OBJ_EVENT_MEDEA, Colchis_Movement_MedeaSanctumReturn2A)
	} else {
		applymovement (OBJ_EVENT_MEDEA, Colchis_Movement_MedeaSanctumReturn2B)
	}
	waitmovement (0)

	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceLeft)
	waitmovement (0)

	//Lecture at Jason
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaSanctumReturn2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)
	closemessage

	//Jason says 
	//Please do not cry beloved Medea
	//lend love the power to hold back your tears
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonMedeaReturn1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)
	

	//IF he has the fleece Jason turns back to say (Sorry, meet me at bulbus later when she calms down)
	if (flag(FLAG_JASON_HAS_SHINYMAREEP)) {
		applymovement (OBJ_EVENT_JASON, Common_Movement_FaceRight)
		waitmovement (0)
		load_field_pic(FIELD_PIC_JASON, 190, 98)
		msgbox(Colchis_Text_JasonMedeaReturn2, MSGBOX_DEFAULT)
		destroy_field_pic (FIELD_PIC_JASON)
		applymovement (OBJ_EVENT_JASON, Common_Movement_FaceLeft)
		waitmovement (0)
	}

	call (Colchis_Script_SetupEscapeFromColchis)

	return
}

script Colchis_Script_SanctumJasonCheckMareep {
	
	specialvar (VAR_RESULT, IsShinyMareepInParty)
	//If no then goto Fight
	if (var(VAR_RESULT) == FALSE) {
		call (Colchis_Script_SanctumJasonBattle)
	//If yes then YES NO
	} else {
		
		load_field_pic(FIELD_PIC_JASON, 190, 98)
		msgbox(Colchis_Text_JasonGiveMeMareep, MSGBOX_DEFAULT)
		destroy_field_pic (FIELD_PIC_JASON)
	
		msgbox(Colchis_Text_GiveJasonMareep, MSGBOX_YESNO)
		//If choose no then goto Fight
		if (var(VAR_RESULT) == FALSE) {
			call (Colchis_Script_SanctumJasonBattle)
		//Else Choose Shiny Mareep
		} else {
			special (ChoosePartyMon)
			waitstate
			//If nothing chosen then go back up top
			if (var(VAR_0x8004) == PARTY_NOTHING_CHOSEN) {
				call (Colchis_Script_SanctumJasonCheckMareep)
			//Else if something chose first
			} else {
				specialvar (VAR_RESULT, ScriptGetPartyMonSpecies)
				//Check if it's Mareep
				if (var(VAR_RESULT) != SPECIES_MAREEP) {
					call (Colchis_Script_SanctumJasonCheckMareep)
				//Next check if it's Shiny
				} else {
					specialvar (VAR_RESULT, ScriptGetPartyMonIsShiny)
					//Not Shiny? Back Up Top
					if (var(VAR_RESULT) != TRUE) {
						call (Colchis_Script_SanctumJasonCheckMareep)
					//YES YOU HAVE SHINY MAREEP
					} else {
						special (BufferMonNickname)
						msgbox (Colchis_Text_GaveShinyMareep, MSGBOX_DEFAULT)
						closemessage
						special (ReleaseChosenMon)
						//Set Flag or Var for Jason has Mareep now (will open Bulbus and the Boat)
						setflag (FLAG_JASON_HAS_SHINYMAREEP)
					}
				}
			}
		}
	}

	return
}

script Colchis_Script_SanctumJasonMareep {

	//Move Player to position
	call (Colchis_Script_SanctumApproachJason)

	//Check if you even have Mareep
	call (Colchis_Script_SanctumJasonCheckMareep)

	return
}

