script Colchis_Script_RingBlocker {
	lockall
	faceplayer

	//Intro - It's a block
	msgbox (Colchis_Text_RingBlockerIntro, MSGBOX_DEFAULT)
	//Check if you have the Key from Aeetes
	checkitem (ITEM_COLCHIS_KEY, 1)
	//If so then do a YESNO open the blocks
	//The key fits!
	//Remove blocks
	//Persist remove blocks
	if (var(VAR_RESULT) == TRUE) {
		msgbox (Colchis_Text_RingBlockerYesNo, MSGBOX_YESNO)
		if (var(VAR_RESULT) == YES) {
			//Key fits
			//The key broke in the lock
			msgbox (Colchis_Text_KeyFits, MSGBOX_DEFAULT)
			closemessage
			removeitem (ITEM_COLCHIS_KEY)
			playse (SE_EFFECTIVE)
			//The barriers dissapear
			hideobjectat (OBJ_EVENT_RINGBARRIER1, MAP_COLCHIS)
			removeobject (OBJ_EVENT_RINGBARRIER1)
			hideobjectat (OBJ_EVENT_RINGBARRIER2, MAP_COLCHIS)
			removeobject (OBJ_EVENT_RINGBARRIER2)
			hideobjectat (OBJ_EVENT_RINGBARRIER3, MAP_COLCHIS)
			removeobject (OBJ_EVENT_RINGBARRIER3)
			//Persist Hide Ring Barriers
			setobjectmovementtype (OBJ_EVENT_RINGBARRIER1, MOVEMENT_TYPE_INVISIBLE)
			setobjectxyperm (OBJ_EVENT_RINGBARRIER1, 1, 3)
			setobjectmovementtype (OBJ_EVENT_RINGBARRIER2, MOVEMENT_TYPE_INVISIBLE)
			setobjectxyperm (OBJ_EVENT_RINGBARRIER2, 2, 3)
			setobjectmovementtype (OBJ_EVENT_RINGBARRIER3, MOVEMENT_TYPE_INVISIBLE)
			setobjectxyperm (OBJ_EVENT_RINGBARRIER3, 3, 3)

			setflag (FLAG_COLCHIS_HIDERINGBARRIER)
			//end
		} else {
			msgbox (Colchis_Text_RingBlockerDecline, MSGBOX_DEFAULT)
			closemessage
		}
	} else {
	   closemessage
	}	   
	
	//fin
	releaseall
	end
}

