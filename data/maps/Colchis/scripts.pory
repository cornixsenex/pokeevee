script Colchis_Script_SetupSanctum {

	//Medea turns
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_FaceRight)
	waitmovement (0)

	//M: Allow me to open the way for you
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaSanctum1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	closemessage
	
	//Medea walks to control panela
	applymovement (OBJ_EVENT_MEDEA, Colchis_Movement_MedeaSanctum1)
	waitmovement (0)
	
	//Open the gate
	playse (SE_EFFECTIVE)
	hideobjectat (OBJ_EVENT_SANCTUM1, MAP_COLCHIS)
	hideobjectat (OBJ_EVENT_SANCTUM2, MAP_COLCHIS)
	removeobject (OBJ_EVENT_SANCTUM1)
    removeobject (OBJ_EVENT_SANCTUM2)

	//Persist hidden object
	setobjectmovementtype (OBJ_EVENT_SANCTUM1, MOVEMENT_TYPE_INVISIBLE)
	setobjectxyperm (OBJ_EVENT_SANCTUM1, 1, 1)
	setobjectmovementtype (OBJ_EVENT_SANCTUM2, MOVEMENT_TYPE_INVISIBLE)
	setobjectxyperm (OBJ_EVENT_SANCTUM2, 3, 1)

	
	//Medea comes back
	applymovement (OBJ_EVENT_MEDEA, Colchis_Movement_MedeaSanctum2)
	waitmovement (0)
	
	//M:I wont do this much for you but the gate is open I can't confront it for you the gate is open
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaSanctum2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)
	
	//J: I couldn't bear to part with you I'm all into you your spell is over me Player you must do this for me bring back Shiny Mareep I will remain here and spend quality time with Medea
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonSanctum1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//Jason turns to and address the player
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceRight)
	waitmovement (0)

	//J: Therefore I trust you to complete this quest. Go forth and bring me back shiny Mareep.
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonSanctum2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	closemessage
	
	//Jason turns back to Medea
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceLeft)
	waitmovement (0)

	//Player sets off
	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_PlayerSanctum1)
	waitmovement (0)
	//Medea faces up
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_FaceUp)
	waitmovement (0)
	//Player faces back
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_FaceDown)
	waitmovement (0)
	//! POINTS
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_ExclamationMark)
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_ExclamationMark)
	waitmovement (0)
	
	//M: One thing you must know Player. Shiny Mareep is guarded by Lugia who is precious to me. You must not bring any harm to her(?) Tho I am fated never to see her again, such is the price of Jason's love, Hecate through me demands she be kept safe
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaSanctum3, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)
	closemessage

	//Medea turns back to Jason
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_FaceRight)
	waitmovement (0)
	
	//Player moves beyond the triggers
	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_PlayerSanctum2)
	waitmovement (0)
	
	//set var to activate the triggers
	setvar (VAR_ARGO_STATE, 11)

	//Persist Jason and Medea outside Sanctum
	setobjectxyperm (OBJ_EVENT_JASON, 31, 60)
	setobjectmovementtype (OBJ_EVENT_JASON, MOVEMENT_TYPE_FACE_LEFT)
	addobject (OBJ_EVENT_JASON)
	showobjectat (OBJ_EVENT_JASON, MAP_COLCHIS)

	setobjectxyperm (OBJ_EVENT_MEDEA, 30, 60)
	setobjectmovementtype (OBJ_EVENT_MEDEA, MOVEMENT_TYPE_FACE_RIGHT)
	addobject (OBJ_EVENT_MEDEA)
	showobjectat (OBJ_EVENT_MEDEA, MAP_COLCHIS)
	releaseall
	//Release
	end
}

