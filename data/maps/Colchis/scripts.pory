			call (Colchis_Script_SetupJail)
		}
	}

	MAP_SCRIPT_ON_LOAD {
		//if (var(VAR_ARGO_STATE) == 7) {
		//	call (Colchis_Script_IntroSetup)
		//}
		////Jason fights at the entrance, battle path
		//if (var(VAR_ARGO_STATE) == 9) {
		//	call (Colchis_Script_IntroDeclinedSetup)
		//}

	}

