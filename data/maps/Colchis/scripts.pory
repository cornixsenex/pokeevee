script Colchis_Script_Aeetes {
	//The goddess through me speaks of things to come
	//Among us there will come a man, Jason,
	//Who traveled here from far beyond the sea,
	//and any man who brings harm onto him
	//will surely Colchis' ruin


	load_field_pic(FIELD_PIC_MEDEA, 190, 98)
	msgbox(Colchis_Text_Medea1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//Aeetes replies:
	//Come forward Jason. Speak with Aeetes.

	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)
	closemessage

	waitmovement (0)
	
	//Enter Jason and Player

	removeobject (OBJ_EVENT_JASON)
	setobjectxyperm (OBJ_EVENT_JASON, 51, 13)
	setobjectmovementtype (OBJ_EVENT_JASON, MOVEMENT_TYPE_FACE_UP)
	addobject (OBJ_EVENT_JASON)
	showobjectat (OBJ_EVENT_JASON, MAP_COLCHIS)	

	applymovement (OBJ_EVENT_JASON, Colchis_Movement_JasonApproachAeetes)
	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_JasonApproachAeetes)
	waitmovement (OBJ_EVENT_ID_PLAYER)

	applymovement (OBJ_EVENT_MEDEA, Colchis_Movement_MedeaFaceAeetes)
	applymovement (OBJ_EVENT_JASON, Colchis_Movement_JasonFaceAeetes)
	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_PlayerFaceAeetes)
	waitmovement (OBJ_EVENT_JASON)


	//Aeetes:
	//Welcome mighty captain of the Argo.
	//We have all heard of your heroic deeds
	//most deserving an honorable feast
	//but first tell me why you've come to Colchis.

	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)

	//J:
	//We come in peace

	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonAeetes, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//A:
	//I asked you why not how!
	//I know you've come to claim shiny Mareep
	//and that if you cannot win bargaining
	//you plan to steal or even kill
	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes3, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)
	
	
	//M: Papa!
	//I beg your majesty to show restraint.
	load_field_pic(FIELD_PIC_MEDEA, 190, 98)
	msgbox(Colchis_Text_Medea2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//A:
	//Silence Medea. Was this not the knave
	//who defiled the sacred temple mount
	//that act alone deserves great punishment
	//and if we should allow shiny Mareep
	//to be stolen Colchis will surely fall
	//therefore I have resolved they must be jailed
	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes4, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)
	closemessage
	
	//Player & Jason ! points
	applymovement (OBJ_EVENT_JASON, Common_Movement_ExclamationMark)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_ExclamationMark)
	waitmovement (0)

	//M:
	//The lesser one before you is guilty
	//alone of attacking sacred Ho-Oh.
	//The lovely Jason's innocent of crime
	//and the goddess through me orders he be
	//kept safe. If your will is set on justice
	//let Jason go free. I'll attend to him...
	load_field_pic(FIELD_PIC_MEDEA, 190, 98)
	msgbox(Colchis_Text_Medea3, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//A:
	//There doom has been pronounced. I have spoken
	//Assemble Royal Guards
	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes5, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)

	//Guards move into position
	applymovement (OBJ_EVENT_ROYALGUARD1, Colchis_Movement_RoyalGuard1)
	applymovement (OBJ_EVENT_ROYALGUARD2, Colchis_Movement_RoyalGuard2)
	applymovement (OBJ_EVENT_ROYALGUARD3, Colchis_Movement_RoyalGuard3)
	applymovement (OBJ_EVENT_ROYALGUARD4, Colchis_Movement_RoyalGuard4)
	waitmovement (OBJ_EVENT_ROYALGUARD1)

	//A:
	//Take them to jail
	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes6, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)

	//They exit

	applymovement (OBJ_EVENT_ID_PLAYER, Colchis_Movement_RoyalGuardExit)
	applymovement (OBJ_EVENT_ROYALGUARD4, Colchis_Movement_RoyalGuardExit)
	applymovement (OBJ_EVENT_ROYALGUARD3, Colchis_Movement_RoyalGuardExit)

	applymovement (OBJ_EVENT_ROYALGUARD1, Colchis_Movement_RoyalGuardExit)
	applymovement (OBJ_EVENT_ROYALGUARD2, Colchis_Movement_RoyalGuardExit)
	applymovement (OBJ_EVENT_JASON, Colchis_Movement_RoyalGuardExit)
	
	//M:
	//NO!
	load_field_pic(FIELD_PIC_MEDEA, 190, 98)
	msgbox(Colchis_Text_Medea4, MSGBOX_DEFAULT)

	waitmovement (0)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//A:
	//Do not weep that your king keeps Colchis safe
	//SCENE
	//goto Jail :D
	load_field_pic(FIELD_PIC_AEETES, 190, 98)
	msgbox(Colchis_Text_Aeetes7, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_AEETES)
	closemessage

	removeobject (OBJ_EVENT_JASON)
	setobjectxyperm (OBJ_EVENT_JASON, 18, 13)
	setobjectmovementtype (OBJ_EVENT_JASON, MOVEMENT_TYPE_FACE_UP)
	addobject (OBJ_EVENT_JASON)
	showobjectat (OBJ_EVENT_JASON, MAP_COLCHIS)	

	call (Colchis_Script_Jail)

	//approaches Aeetes and speaks: 
	//Aeetes: step forward Jason
	//Enter Jason and Player where camera ended pan :)


	//msgbox("END")
	//closemessage


	return
}

script Colchis_Script_Jail {

	//Move to scene	
	special (RemoveCameraObject)

	msgbox ("TEST", MSGBOX_DEFAULT)
	closemessage

	warpsilent (MAP_COLCHIS, 18, 10)
	waitstate

	//Jason bangs on the door
	//It's not use we're stuck
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonPrison1, MSGBOX_DEFAULT)
	closemessage
	destroy_field_pic (FIELD_PIC_JASON)

	//Jason Walks up
	applymovement (OBJ_EVENT_JASON, Colchis_Movement_JasonPrisonApproachPlayer)
	waitmovement (0)
	
	//The quest is lost. I fear I've failed us both
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonPrison2, MSGBOX_DEFAULT)
	closemessage
	destroy_field_pic (FIELD_PIC_JASON)
	
	//He turns, ! Point
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceDown)
	waitmovement (0)
	applymovement (OBJ_EVENT_JASON, Common_Movement_ExclamationMark)
	waitmovement (0)

	//Place Medea
	removeobject (OBJ_EVENT_MEDEA)
	setobjectxyperm (OBJ_EVENT_MEDEA, 18, 16)
	setobjectmovementtype (OBJ_EVENT_MEDEA, MOVEMENT_TYPE_FACE_UP)
	addobject (OBJ_EVENT_MEDEA)
	showobjectat (OBJ_EVENT_MEDEA, MAP_COLCHIS)	
	
	//Replace the tiles
	playse (SE_EFFECTIVE)
	setmetatile (18, 14, METATILE_Colchis_JailDoorOpen_Bottom, FALSE)
	setmetatile (18, 15, METATILE_Colchis_JailDoorOpen_Top, FALSE)
	
	//Enter Medea
	applymovement (OBJ_EVENT_MEDEA, Common_Movement_MedeaPrisonApproachPlayer)
	waitmovement (0)

	//I've come to save you 
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaPrison1, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//You are a witch! Why should I now trust you?
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonPrison3, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//Noone more beautiful love lets go
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaPrison2, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//Your words do move me but I cannot leave without Mareep
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonPrison4, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//Anything for you my love
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaPrison3, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	closemessage 

	//Jason Faces Player
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceUp)
	waitmovement (0)

	//We should trust her lets go
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonPrison5, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)
	closemessage 

	//Turns back to Medea
	applymovement (OBJ_EVENT_JASON, Common_Movement_FaceDown)
	waitmovement (0)

	//What is your plan we cant take them all
	load_field_pic(FIELD_PIC_JASON, 190, 98)
	msgbox(Colchis_Text_JasonPrison6, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_JASON)

	//I've put my spell on them and they won't wake so long as you travel with me lets go
	load_field_pic(FIELD_PIC_MEDEA 190, 98)
	msgbox(Colchis_Text_MedeaPrison4, MSGBOX_DEFAULT)
	destroy_field_pic (FIELD_PIC_MEDEA)

	//Movevement to Sanctum

	applymovement (OBJ_EVENT_MEDEA, Common_Movement_MedeaPrisonExit)
	applymovement (OBJ_EVENT_JASON, Common_Movement_JasonPrisonExit)
	applymovement (OBJ_EVENT_ID_PLAYER, Common_Movement_PlayerPrisonExit)
	waitmovement (OBJ_EVENT_ID_PLAYER)

	return

}



	

